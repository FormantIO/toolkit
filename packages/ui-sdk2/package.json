    "@types/react-dom": "^17"
  },
  "types": "./dist/types/main.d.ts",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.6.1",
    "@mui/utils": "^5.6.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typedoc": "^0.22.13"
  }
}
