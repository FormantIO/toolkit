{
  "name": "@formant/ui-sdk",
  "description": "A library for getting UI from Formant",
  "repository": {
    "type": "git",
    "url": "https://github.com/formantio/toolkit.git"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "homepage": "https://github.com/formantio/toolkit",
  "main": "./dist/ui-sdk.es.js",
  "exports": {
    ".": "./dist/ui-sdk.es.js"
  },
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --port 9146",
