{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": [
    "../src/**/*"
  ],
  "exclude": [
    "../node_modules",
    "../src/**/*.spec.ts",
    "../src/**/*.test.ts",
    "../src/**/*.spec.tsx",
    "../src/**/*.test.tsx"
  ]
}
