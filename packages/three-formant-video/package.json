  },
  "devDependencies": {
    "typescript": "^4.3.2",
    "vite": "^2.5.4"
  },
  "dependencies": {
    "@formant/data-sdk": "^0.0.27",
    "@formant/ui-sdk-realtime-player-core": "0.0.0",
    "@formant/ui-sdk-realtime-player-core-worker": "^0.0.0",
    "@types/three": "^0.131.0",
    "three": "^0.132.2"
  },
  "types": "./dist/types/main.d.ts"
}
