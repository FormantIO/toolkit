{
  "name": "@formant/three-formant-video",
  "files": [
    "dist"
  ],
  "main": "./dist/three-formant-video.umd.js",
  "module": "./dist/three-formant-video.es.js",
  "exports": {
    ".": {
      "import": "./dist/three-formant-video.es.js",
      "require": "./dist/three-formant-video.umd.js"
    }
  },
  "version": "0.0.2",
  "scripts": {
    "dev": "vite --port 9146",
