{
  "name": "@formant/universe",
  "description": "A library visualizing 3D universes on Formant",
  "repository": {
    "type": "git",
    "url": "https://github.com/formantio/toolkit.git"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "homepage": "https://github.com/formantio/toolkit",
  "main": "./dist/universe.umd.js",
  "module": "./dist/universe.es.js",
  "exports": {
    ".": {
      "import": "./dist/universe.es.js",
      "require": "./dist/universe.umd.js"
    }
  },
  "version": "0.0.13",
  "scripts": {
    "dev": "vite --port 9146",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "types": "tsc src/*.tsx src/*.ts src/**/*.tsx src/**/*.ts --jsx react --moduleResolution node --target ESNext --lib ESNext,DOM --allowSyntheticDefaultImports --declaration --emitDeclarationOnly --downlevelIteration --outDir dist/types",
    "docs": "typedoc src/main.ts --theme default --out ../../docs/universe/"
  },
  "devDependencies": {
    "@types/offscreencanvas": "^2019.6.4",
    "@types/react-dom": "^18.0.0",
    "@types/styled-components": "^5.1.25",
    "@types/three": "^0.125.2",
    "react-dom": "^18.0.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5"
  },
  "types": "./dist/types/universe/src/main.d.ts",
  "dependencies": {
    "@formant/ui-sdk": "0.0.13",
    "@types/react": "^18.0.8",
    "@types/uuid": "^8.3.4",
    "classnames": "^2.3.1",
    "geolib": "^3.3.3",
    "react": "^18.0.0",
    "react-mosaic-component": "^5.1.0",
    "react-use-measure": "^2.1.1",
    "style-inject": "^0.3.0",
    "styled-components": "^5.3.5",
    "three": "^0.125.2",
    "uuid": "^8.3.2"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "peerDependencies": {
    "three": "^0.125.2"
  }
}
