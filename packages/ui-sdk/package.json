{
  "name": "@formant/ui-sdk",
  "description": "A library for getting UI from Formant",
  "repository": {
    "type": "git",
    "url": "https://github.com/formantio/toolkit.git"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "homepage": "https://github.com/formantio/toolkit",
  "main": "./dist/ui-sdk.es.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/ui-sdk/src/main.d.ts",
        "default": "./dist/ui-sdk.es.js"
      },
      "require": {
        "types": "./dist/types/ui-sdk/src/main.d.ts",
        "default": "./dist/ui-sdk.umd.js"
      }
    }
  },
  "version": "0.0.55",
  "scripts": {
    "dev": "vite --port 5173",
    "build": "vite build",
    "serve": "vite preview",
    "types": "tsc src/**/*.ts src/**/*.tsx --jsx react --lib ESNext,DOM --allowSyntheticDefaultImports --declaration --emitDeclarationOnly --experimentalDecorators --downlevelIteration --outDir dist/types",
    "docs": "typedoc src/main.ts --theme default --out ../../docs/ui-sdk/"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@emotion/babel-plugin": "^11.10.6",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "babel-loader": "^8.2.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typedoc": "^0.22.13",
    "typescript": "^4.6.3",
    "vite": "^4.1.0"
  },
  "types": "./dist/types/ui-sdk/src/main.d.ts",
  "dependencies": {
    "@emotion/css": "^11.10.6",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@formant/ui-sdk-realtime-player": "^0.0.9",
    "@mui/material": "^5.6.1",
    "@mui/utils": "^5.6.1",
    "@types/lodash": "^4.14.182",
    "@vitejs/plugin-react": "^3.1.0",
    "chart.js": "^4.2.0",
    "classnames": "^2.3.1",
    "grpc-web": "1.2.1",
    "lodash": "^4.17.21",
    "react-chartjs-2": "^5.2.0",
    "react-device-detect": "^2.2.2"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@formant/data-sdk": "^1.0.0"
  }
}
